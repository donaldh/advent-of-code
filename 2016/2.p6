#!/usr/bin/env perl6

use v6;

my $input = "



";

my @keypad[5;5] =
(Nil, Nil,  1,  Nil, Nil),
(Nil,  2,   3,    4, Nil),
(  5,  6,   7,    8,   9),
(Nil, 'A', 'B', 'C', Nil),
(Nil, Nil, 'D', Nil, Nil);

my $x = 1;
my $y = 1;

for $input.lines {
    for .comb {
        when 'U' {
            $y-- if $y > 0 and @keypad[$y - 1; $x];
        }
        when 'D' {
            $y++ if $y < 4 and @keypad[$y + 1; $x];
        }
        when 'L' {
            $x-- if $x > 0 and @keypad[$y; $x - 1];
        }
        when 'R' {
            $x++ if $x < 4 and @keypad[$y; $x + 1];
        }
    }
    print @keypad[$y; $x];
}

say '';
